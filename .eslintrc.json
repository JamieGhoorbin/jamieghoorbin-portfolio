{
  "env": {
    "browser": true,
    "es2020": true
    // "jest": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-console": "warn",
    "import/first": "error"
    // "no-unused-variable": ["warn", {"ignore-pattern": "^_"}]
  }
}
